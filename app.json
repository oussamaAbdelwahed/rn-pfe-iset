{
    "expo": {
        "name": "ja",
        "slug": "JA",
        "privacy": "public",
        "sdkVersion": "32.0.0",
        "platforms": ["ios", "android"],
        "version": "1.0.0",
        "orientation": "portrait",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#f5f6fa"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "package": "com.jeuneafrique.JARN",
            "permissions": ["CAMERA", "CAMERA_ROLL"]
        }
    }
}
